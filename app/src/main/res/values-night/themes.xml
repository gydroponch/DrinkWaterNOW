<resources xmlns:tools="http://schemas.android.com/tools">
    <style name="TimePickerFragmentTheme" parent="@style/Theme.AppCompat.Light.Dialog">
        <item name="colorAccent">@color/darkerBlueForStatusBar</item>
        <item name="colorControlNormal">@color/blueForStatusBar</item>
    </style>
    <style name="Theme.drinkwaternow" parent="Theme.MaterialComponents.DayNight">
        <!-- Primary brand color. -->
        <item name="windowNoTitle">true</item>
        <item name="colorPrimary">@color/blueForStatusBar</item>
        <item name="colorPrimaryVariant">@color/darkerBlueForStatusBar</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">@color/darkerBlueForStatusBar</item>
        <!-- Customize your theme here. -->
    </style>
</resources>